    "/artists/": {
      "get": {
        "tags": [
          "artistas"
        ],
        "summary": "realiza a listagem dos artistas por paginas",
        "description": "A rota retorna todos os artistas conforme definido página e limite nos parametros",
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "description": "numero da pagina",
            "required": true,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "limit",
            "in": "query",
            "description": "limite de itens por pagina",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "artistas dessa página:",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/artist"
                  }
                }
              }
            }
          },
          "400": {
            "description": "limite invalido, precisa ser 5 , 10 ou 30",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "limite invalido, precisa ser 5 , 10 ou 30"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "artistas"
        ],
        "summary": "adição de artistas",
        "description": "rota usada para cadastrar artistas na base de dados",
        "responses": {
          "201": {
            "description": "artista adicionado com sucesso.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/artist"
                  }
                }
              }
            }
          },
          "400": {
            "description": "artista não adicionado",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "houve um erro com o artista enviado"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Sem permisão",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "faça o login para realizar esta ação"
                    }
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/artist"
              },
              "examples": {
                "artist": {
                  "value": {
                    "name": "Michael Jackson",
                    "genre": "Pop"
                  }
                }
              }
            }
          }
        }
      }
    },
