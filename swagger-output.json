        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/artist"
              },
              "examples": {
                "artist": {
                  "value": {
                    "name": "Elvis Presley",
                    "genre": "Rock"
                  }
                }
              }
            }
          }
        }
      },
      "delete": {
